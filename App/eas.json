{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"         
      },
      "ios": {
        "simulator": false         
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"  
      },
      "ios": {
        "simulator": false         
      },
      "env": {
        "API_URL": "https://autofixapp.autofix.lat/api",
        "GOOGLE_API_KEY": "AIzaSyDwkMf1ECQfbEWLVvYC1UJ_HUNDI0_aDCw",
        "GOOGLE_API_KEY_IOS": "AIzaSyCCGn5gqx_VmrAq0izYqyUYbbFrGVAlcE8"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./keys/google-play-service-account.json"
      },
      "ios": {
        "ascAppId": "TU_APPLE_APP_ID"
      }
    }
  }
}
